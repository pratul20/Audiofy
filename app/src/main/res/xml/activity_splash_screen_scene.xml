<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="2000">
       <KeyFrameSet>
           <KeyAttribute
               motion:motionTarget="@+id/textView4"
               motion:framePosition="0"
               android:alpha="0" />
           <KeyAttribute
               motion:motionTarget="@+id/textView4"
               motion:framePosition="0"
               android:translationY="60dp" />
           <KeyAttribute
               motion:motionTarget="@+id/textView4"
               motion:framePosition="0"
               android:translationY="30dp" />
       </KeyFrameSet>
    </Transition>

    <ConstraintSet>
        <Constraint
            motion:layout_constraintBottom_toBottomOf="@+id/textView4"
            android:id="@+id/lottieAnimationView" />
        <Constraint
            motion:layout_constraintBottom_toBottomOf="@+id/textView4"
            android:id="@+id/lottieAnimationView2" />
        <Constraint
            motion:layout_constraintBottom_toTopOf="@+id/textView4"
            android:id="@+id/lottieAnimationView3" />
        <Constraint
            motion:layout_constraintBottom_toBottomOf="@+id/textView4"
            android:id="@+id/lottieAnimationView4" />
        <Constraint
            motion:layout_editor_absoluteY="407dp"
            android:id="@+id/lottieAnimationView5"
            android:layout_width="34dp"
            android:layout_height="38dp"
            motion:layout_constraintStart_toStartOf="@+id/textView4"
            motion:layout_constraintEnd_toEndOf="@+id/textView4" />
        <Constraint
            motion:layout_constraintBottom_toBottomOf="parent"
            android:id="@+id/lottieAnimationView6" />
        <Constraint
            motion:layout_constraintBottom_toBottomOf="parent"
            android:id="@+id/lottieAnimationView7" />
        <Constraint
            android:id="@+id/lottieAnimationView8"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="296dp"
            motion:layout_constraintHorizontal_bias="0.0" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
    </ConstraintSet>
</MotionScene>